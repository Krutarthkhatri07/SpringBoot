<body>
	username:<input type="text" id="uname" onchange="fun()" />
	<div id="error"></div>
	password:<input type="password" id="pass" />
	<script type="text/javascript">
		function fun() {

			var htp = new XMLHttpRequest();
			var uname= document.getElementById("uname");
			var un=uname.value;

			htp.onreadystatechange = function() {

				if (htp.readyState == 4) {
					
					var div= document.getElementById("error");
					var pass= document.getElementById("pass");
					div.innerHTML = "";
					//console.log(uname.value);
					var json = JSON.parse(htp.responseText);
					
					for(var i=0;i<json.length;i++)
						{
							var data=json[i];
						
							var cname=uname.value;
							console.log(cname);
							var jname=data.name;
							if(jname.localeCompare("true") == 0)
								{
									div.innerHTML ="user name alreadt exists...!!!";
								}
							
						}
				}

			}
			htp.open("get", "json.xx?username1="+un, true);
			htp.send();

		}
	</script>


</body>
